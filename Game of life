fr = open('input.gol','r')

def create2Dmatrix(width, height):
    matrix = []
    temp = []
    for y in range(height):
        temp = []
        for x in range(width):
            temp.append(0)
            matrix.append(temp)
    return matrix

def processfile(matrix):
    x = 0
    y = 0
    for row in fr:
        for char in row:
            if char =='1':
                matrix[y][x]=1
            x+=1
        y+=1

